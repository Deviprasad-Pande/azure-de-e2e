{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "WinArcDataFactory"
		},
		"LSAzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LSAzureDataLakeStorage'"
		},
		"LSAzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LSAzureSqlDatabase'"
		},
		"LSAzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccountde.dfs.core.windows.net/"
		},
		"LSAzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "winarc.database.windows.net"
		},
		"LSAzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SQLDatabaseWinArc"
		},
		"LSAzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "user"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "WinArcDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LSAzureDataLakeStorage_accountKey": {
						"value": "[parameters('LSAzureDataLakeStorage_accountKey')]"
					},
					"LSAzureSqlDatabase_password": {
						"value": "[parameters('LSAzureSqlDatabase_password')]"
					},
					"LSAzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('LSAzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"LSAzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('LSAzureSqlDatabase_properties_typeProperties_server')]"
					},
					"LSAzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('LSAzureSqlDatabase_properties_typeProperties_database')]"
					},
					"LSAzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('LSAzureSqlDatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}